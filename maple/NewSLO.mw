<Group labelreference="L1091" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">mvdnorm := proc(X::Vector,Mu::Vector,InvCov::Matrix)
  local XMu;
  XMu := X - Mu;
  exp(-LinearAlgebra:-BilinearForm(XMu,XMu,InvCov,conjugate=false)/2)
  * LinearAlgebra:-Determinant(InvCov)^(1/2)
  * (2*Pi)^(-LinearAlgebra:-Dimension(X)/2)
end proc;</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
</Output>
</Group>
<Group labelreference="L1097" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">BindMLebesgue := proc(n::nonnegint,X::name,m)
  if n &gt; 0 then
    BindMLebesgue(n-1,X, Bind(Lebesgue(),X[n], m))
  else
    m
  end if
end proc;</Text-field>
</Input>
